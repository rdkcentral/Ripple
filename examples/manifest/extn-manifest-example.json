{
    "default_path": "/usr/lib/rust/",
    "default_extension": "so",
    "extns": [
        {
            "path": "libthunder",
            "symbols": [
                {
                    "id": "ripple:channel:device:thunder",
                    "uses": [
                        "config"
                    ],
                    "fulfills": [
                        "device:info",
                        "device:windowmanager",
                        "device:browser"
                    ]
                }
            ]
        },
        {
            "path": "liblauncher",
            "symbols": [
                {
                    "id": "ripple:channel:launcher:internal",
                    "uses": [
                        "config"
                    ],
                    "fulfills": [
                        "launcher"
                    ]
                }
            ]
        }
    ],
    "required_contracts": [
        "launcher",
        "config",
        "device:info",
        "device:windowmanager",
        "device:browser",
        "device:wifi"        
    ]
}