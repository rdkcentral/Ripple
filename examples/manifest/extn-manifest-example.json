{
    "default_path": "/usr/lib/rust/",
    "default_extension": "so",
    "timeout": 2000,
    "extns": [
        {
            "path": "libthunder",
            "symbols": [
                {
                    "id": "ripple:channel:device:thunder",
                    "uses": [
                        "config",
                        "app_events",
                        "rpc",
                        "ripple_context",
                        "operational_metric_listener"
                    ],
                    "fulfills": [
                        "device_info",
                        "window_manager",
                        "browser",
                        "wifi",
                        "device_persistence",
                        "remote_accessory",
                        "app_events",
                        "input.device_events",
                        "voice_guidance.device_events",
                        "audio.device_events"
                    ],
                    "config": {
                        "rdk_telemetry": "true"
                    }
                }
            ]
        },
        {
            "path": "libdistributor_general",
            "symbols": [
                {
                    "id": "ripple:channel:distributor:general",
                    "uses": [
                        "config"
                    ],
                    "fulfills": [
                        "account_session",
                        "advertising",
                        "privacy_settings",
                        "session_token"
                    ]
                }
            ]
        }
    ],
    "required_contracts": [
        "rpc",
        "lifecycle_management",
        "device_info",
        "window_manager",
        "browser",
        "account_session",
        "wifi",
        "device_persistence",
        "remote_accessory",
        "advertising",
        "privacy_settings",
        "session_token"
    ],
    "rpc_aliases": {
        "device.model": [
            "custom.model"
        ]
    },
    "rules_path": [
        "/etc/ripple.common.rules.json"
    ]
}