{
    "default_path": "/usr/lib/rust/",
    "default_extension": "so",
    "timeout": 2000,
    "extns": [
        {
            "path": "libthunder",
            "symbols": [
                {
                    "id": "ripple:channel:device:thunder",
                    "uses": [
                        "config"
                    ],
                    "fulfills": [
                        "device_info",
                        "window_manager",
                        "browser",
                        "wifi",
                        "device_events",
                        "device_persistence",
                        "remote_accessory",
                        "local.storage",
                        "input.device_events",
                        "voice_guidance.device_events",
                        
                        "audio.device_events",
                        "remote_feature_control"
                    ]
                }
            ]
        },
        {
            "path": "libdistributor_general",
            "symbols": [
                {
                    "id": "ripple:channel:distributor:general",
                    "uses": [
                        "config"
                    ],
                    "fulfills": [
                        "permissions",
                        "account.session",
                        "session.token",
                        "root.session",
                        "device.session"
                    ]
                }
            ]
        },
        {
            "path": "libmock_device",
            "symbols": [
                {
                    "id": "ripple:channel:device:mock_device",
                    "config": {
                        "mock_data_file": "mock-device.json",
                        "activate_all_plugins": "true"
                    },
                    "uses": [
                        "config"
                    ],
                    "fulfills": []
                },
                {
                    "id": "ripple:extn:jsonrpsee:mock_device",
                    "uses": [
                        "ripple:channel:device:mock_device"
                    ],
                    "fulfills": [
                        "json_rpsee"
                    ]
                }
            ]
        }
    ],
    "required_contracts": [
        "rpc",
        "lifecycle_management",
        "device_info",
        "window_manager",
        "browser",
        "permissions",
        "account_session",
        "wifi",
        "device_events",
        "device_persistence",
        "remote_accessory",
        "privacy_settings",
        "session_token",
        "account.session"
    ],
    "rpc_aliases": {
        "device.model": [
            "custom.model"
        ]
    }
}